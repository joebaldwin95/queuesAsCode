{
  "$id": "http://example.com/example.json",
  "type": "object",
  "properties": {
    "queues": {
      "$id": "/properties/queues",
      "type": "array",
      "items": {
        "$id": "/properties/queues/items",
        "type": "object",
        "properties": {
          "name": {
            "$id": "/properties/queues/items/properties/name",
            "type": "string",
            "title": "The Name Schema ",
            "default": "",
            "examples": [
              "testytest1"
            ]
          },
          "fifo": {
            "$id": "/properties/queues/items/properties/fifo",
            "type": "boolean",
            "title": "The Fifo Schema ",
            "default": false,
            "examples": [
              false
            ]
          },
          "encrypted": {
            "$id": "/properties/queues/items/properties/encrypted",
            "type": "boolean",
            "title": "The Encrypted Schema ",
            "default": false,
            "examples": [
              true
            ]
          },
          "dlq": {
            "$id": "/properties/queues/items/properties/dlq",
            "type": "boolean",
            "title": "The Dlq Schema ",
            "default": false,
            "examples": [
              true
            ]
          },
          "dlqName": {
            "$id": "/properties/queues/items/properties/dlqName",
            "type": "string",
            "title": "The Dlqname Schema ",
            "default": "",
            "examples": [
              "1010"
            ]
          },
          "maxDelivery": {
            "$id": "/properties/queues/items/properties/maxDelivery",
            "type": "integer",
            "title": "The Maxdelivery Schema ",
            "default": 0,
            "examples": [
              3
            ]
          },
          "orgId": {
            "$id": "/properties/queues/items/properties/orgId",
            "type": "string",
            "title": "The Orgid Schema ",
            "default": "",
            "examples": [
              "8d67-25c6a08eac2e"
            ]
          },
          "envId": {
            "$id": "/properties/queues/items/properties/envId",
            "type": "string",
            "title": "The Envid Schema ",
            "default": "",
            "examples": [
              "4ac9-9756-28b586fa4751"
            ]
          }
        },
        "required": [
          "name",
          "fifo",
          "encrypted",
          "dlq",
          "orgId",
          "envId"
        ]
      }
    }
  }
}